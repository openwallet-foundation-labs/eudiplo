{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/openwallet-foundation-labs/eudiplo/refs/heads/main/schemas/TokenResponse.schema.json",
  "title": "TokenResponse",
  "type": "object",
  "properties": {
    "access_token": {
      "type": "string"
