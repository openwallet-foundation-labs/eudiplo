{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/openwallet-foundation-labs/eudiplo/refs/heads/main/schemas/UpdateKeyDto.schema.json",
  "title": "UpdateKeyDto",
  "type": "object",
  "properties": {
    "kmsProvider": {
      "type": "string",
      "description": "KMS provider name to use for this key. Defaults to the configured default.",
      "example": "db"
    },
    "id": {
      "type": "string",
      "description": "Unique identifier for the key."
    },
    "description": {
      "type": "string",
      "description": "Description of the key."
    }
  },
  "required": [
    "id"
  ]
}