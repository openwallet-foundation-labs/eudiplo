{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/openwallet-foundation-labs/eudiplo/refs/heads/main/schemas/VaultKmsConfigDto.schema.json",
  "title": "VaultKmsConfigDto",
  "type": "object",
  "properties": {
    "vaultUrl": {
      "type": "string",
      "description": "URL of the HashiCorp Vault instance. Supports ${ENV_VAR} placeholders.",
      "example": "${VAULT_URL}"
    },
    "vaultToken": {
      "type": "string",
      "description": "Authentication token for HashiCorp Vault. Supports ${ENV_VAR} placeholders.",
      "example": "${VAULT_TOKEN}"
    }
  },
  "required": [
    "vaultUrl",
    "vaultToken"
  ]
}