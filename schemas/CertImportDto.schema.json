{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/openwallet-foundation-labs/eudiplo/refs/heads/main/schemas/CertImportDto.schema.json",
  "title": "CertImportDto",
  "type": "object",
  "properties": {
    "type": {
      "type": "array",
      "description": "Certificate usage type(s)",
      "example": [
        "signing"
      ],
      "items": {
        "type": "string",
        "enum": [
          "access",
          "signing"
        ]
      }
    },
    "keyId": {
      "type": "string",
      "description": "Key ID of the certificate's private key."
    },
    "id": {
      "type": "string",
      "description": "Unique identifier for the key."
    },
    "crt": {
      "type": "string",
      "description": "Certificate in PEM format."
    },
    "description": {
      "type": "string",
      "description": "Description of the key."
    }
  },
  "required": [
    "type",
    "keyId",
    "id",
    "crt"
  ]
}