{
    "name": "eudiplo",
    "version": "0.0.1",
    "description": "a middleware to interact with the EUDI Wallet",
    "author": "Mirko Mollik <mirkomollik@gmail.com>",
    "private": true,
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/openwallet-foundation-labs/eudiplo.git"
    },
    "bugs": {
        "url": "https://github.com/openwallet-foundation-labs/eudiplo/issues"
    },
    "engines": {
        "node": ">=22.0.0",
        "pnpm": ">=8.0.0"
    },
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "format:doc": "prettier --write \"docs/**/*.md\"",
        "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:cov": "vitest run --coverage",
        "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
        "test:e2e": "vitest run --coverage --config ./test/vitest.config.ts",
        "test:e2e:watch": "vitest --config ./test/vitest.config.ts",
        "registrar": "npx @hey-api/openapi-ts -i https://funke-wallet.de/-json -o ./src/registrar/generated -c @hey-api/client-fetch",
        "swagger": "SWAGGER_JSON=true nest start",
        "compodoc": "compodoc -p tsconfig.doc.json --theme material -d docs/compodoc",
        "compodoc:serve": "compodoc -p tsconfig.doc.json --theme material -d docs/compodoc -s -r 3001 -w",
        "doc:watch": "pnpm run swagger && mkdocs serve",
        "doc:build": "pnpm run swagger && pnpm run compodoc && mkdocs build --strict",
        "doc:init": "pnpm run swagger && pnpm run compodoc && mike deploy --update-aliases main latest-dev && mike set-default main",
        "doc:serve-versions": "mike serve",
        "doc:deploy-dev": "pnpm run swagger && pnpm run compodoc && mike deploy dev",
        "doc:deploy-major": "pnpm run swagger && pnpm run compodoc && mike deploy --update-aliases",
        "doc:list-versions": "mike list",
        "doc:delete-version": "mike delete",
        "import": "ts-node ./test/import/import.ts"
    },
    "dependencies": {
        "@badgateway/oauth2-client": "^3.2.0",
        "@nestjs/axios": "^4.0.0",
        "@nestjs/common": "^11.1.3",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.1.3",
        "@nestjs/event-emitter": "^3.0.1",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.1.3",
        "@nestjs/schedule": "^6.0.0",
        "@nestjs/serve-static": "^5.0.3",
        "@nestjs/swagger": "^11.2.0",
        "@nestjs/terminus": "^11.0.0",
        "@nestjs/typeorm": "^11.0.0",
        "@openid4vc/oauth2": "0.3.0-alpha-20250602121005",
        "@openid4vc/openid4vci": "0.3.0-alpha-20250602121005",
        "@openid4vc/openid4vp": "0.3.0-alpha-20250602121005",
        "@openid4vc/utils": "^0.3.0-alpha-20250602121005",
        "@sd-jwt/crypto-nodejs": "^0.14.0",
        "@sd-jwt/jwt-status-list": "^0.14.0",
        "@sd-jwt/sd-jwt-vc": "^0.14.0",
        "@sd-jwt/types": "^0.14.0",
        "axios": "^1.10.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "joi": "^17.13.3",
        "jose": "^6.0.11",
        "jwks-rsa": "^3.2.0",
        "mysql2": "^3.14.1",
        "nestjs-pino": "^4.4.0",
        "passport-jwt": "^4.0.1",
        "pg": "^8.16.3",
        "pino-http": "^10.5.0",
        "pino-pretty": "^13.0.0",
        "qrcode": "^1.5.4",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "sqlite3": "^5.1.7",
        "typeorm": "^0.3.25",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@codecov/vite-plugin": "^1.9.1",
        "@codedependant/semantic-release-docker": "^5.1.1",
        "@compodoc/compodoc": "^1.1.26",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.30.0",
        "@hey-api/openapi-ts": "^0.78.3",
        "@nestjs/cli": "^11.0.7",
        "@nestjs/schematics": "^11.0.5",
        "@nestjs/testing": "^11.1.3",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^11.0.3",
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.12.7",
        "@types/express": "^5.0.3",
        "@types/node": "^22.15.34",
        "@types/supertest": "^6.0.3",
        "@vitest/coverage-v8": "^3.2.4",
        "dotenv": "^17.2.0",
        "eslint": "^9.30.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.5.1",
        "express": "^5.1.0",
        "globals": "^16.2.0",
        "prettier": "^3.6.2",
        "semantic-release": "^24.2.6",
        "semantic-release-docker": "^2.2.0",
        "source-map-support": "^0.5.21",
        "supertest": "^7.1.1",
        "testcontainers": "^11.2.1",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.35.0",
        "undici": "^7.11.0",
        "unplugin-swc": "^1.5.5",
        "vitest": "^3.2.4"
    },
    "packageManager": "pnpm@10.13.1"
}