    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "format:doc": "prettier --write \"docs/**/*.md\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "registrar": "npx @hey-api/openapi-ts -i https://funke-wallet.de/-json -o ./src/registrar/generated -c @hey-api/client-fetch",
        "end-rp": "ts-node examples/end-rp.ts",
        "swagger": "SWAGGER_JSON=true nest start",
        "compodoc": "compodoc -p tsconfig.json --theme material -d docs/compodoc",
        "compodoc:serve": "compodoc -p tsconfig.json --theme material -d docs/compodoc -s -r 3001",
        "doc:watch": "pnpm run swagger && mkdocs serve"
    },
