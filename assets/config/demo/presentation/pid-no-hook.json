{
  "id": "pid-no-hook",
  "description": "Presentation ID",
  "dcql_query": {
    "credentials": [
      {
        "id": "pid",
        "format": "dc+sd-jwt",
        "meta": {
          "vct_values": [
            "<TENANT_URL>/credentials-metadata/vct/pid"
          ]
        },
        "claims": [
          {
            "path": [
              "address",
              "locality"
            ]
          }
        ],
        "trusted_authorities": [
          {
            "type": "etsi_tl",
            "values": [
              "<TENANT_URL>/trust-list/580831bc-ef11-43f4-a3be-a2b6bf1b29a3"
            ]
          }
        ]
      }
    ]
  }
}