{
  "id": "pid-no-hook",
  "description": "Presentation ID",
  "dcql_query": {
    "credentials": [
      {
        "id": "pid",
        "format": "dc+sd-jwt",
        "meta": {
          "vct_values": [
            "<PUBLIC_URL>/credentials-metadata/vct/pid"
          ]
        },
        "claims": [
          {
            "path": [
              "address",
              "locality"
            ]
          }
        ],
        "trusted_authorities": [
          {
            "type": "etsi_tl",
            "values": [
              "<PUBLIC_URL>/trust-list/039af178-3ca0-48f4-a2e4-7b1209f31376"
            ]
          }
        ]
      }
    ]
  }
}