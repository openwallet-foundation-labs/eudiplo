{
  "id": "pid",
  "description": "Presentation ID",
  "dcql_query": {
    "credentials": [
      {
        "id": "pid",
        "format": "dc+sd-jwt",
        "meta": {
          "vct_values": [
            "<PUBLIC_URL>/credentials-metadata/vct/pid"
          ]
        },
        "claims": [
          {
            "path": [
              "address",
              "locality"
            ]
          }
        ],
        "trusted_authorities": [
          {
            "type": "etsi_tl",
            "values": [
              "<PUBLIC_URL>/trust-list/580831bc-ef11-43f4-a3be-a2b6bf1b29a3"
            ]
          }
        ]
      }
    ]
  },
  "webhook": {
    "url": "http://localhost:8787/consume",
    "auth": {
      "type": "apiKey",
      "config": {
        "headerName": "x-api-key",
        "value": "foo-bar"
      }
    }
  }
}