{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "ES2022",
    "moduleResolution": "bundler",
    "target": "ES2022",
    "lib": ["ES2022", "dom", "dom.iterable"],
    "outDir": "../../out-tsc/app",
    "types": [],
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "files": ["src/main.ts"],
  "include": ["src/**/*.d.ts"]
}
