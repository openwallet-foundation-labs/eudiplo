#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mirko/Projects/eudiplo/node_modules/.pnpm/@hey-api+openapi-ts@0.80.5_magicast@0.3.5_typescript@5.9.2/node_modules/@hey-api/openapi-ts/bin/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/@hey-api+openapi-ts@0.80.5_magicast@0.3.5_typescript@5.9.2/node_modules/@hey-api/openapi-ts/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/@hey-api+openapi-ts@0.80.5_magicast@0.3.5_typescript@5.9.2/node_modules/@hey-api/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/@hey-api+openapi-ts@0.80.5_magicast@0.3.5_typescript@5.9.2/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mirko/Projects/eudiplo/node_modules/.pnpm/@hey-api+openapi-ts@0.80.5_magicast@0.3.5_typescript@5.9.2/node_modules/@hey-api/openapi-ts/bin/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/@hey-api+openapi-ts@0.80.5_magicast@0.3.5_typescript@5.9.2/node_modules/@hey-api/openapi-ts/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/@hey-api+openapi-ts@0.80.5_magicast@0.3.5_typescript@5.9.2/node_modules/@hey-api/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/@hey-api+openapi-ts@0.80.5_magicast@0.3.5_typescript@5.9.2/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@hey-api/openapi-ts/bin/index.cjs" "$@"
else
  exec node  "$basedir/../@hey-api/openapi-ts/bin/index.cjs" "$@"
fi
