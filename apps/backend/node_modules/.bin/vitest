#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mirko/Projects/eudiplo/node_modules/.pnpm/vitest@3.2.4_@types+node@24.2.0_jiti@2.5.1_msw@2.10.4_@types+node@24.2.0_typescript@5.9_0c37c91df05756866e938c0c146bf3a9/node_modules/vitest/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/vitest@3.2.4_@types+node@24.2.0_jiti@2.5.1_msw@2.10.4_@types+node@24.2.0_typescript@5.9_0c37c91df05756866e938c0c146bf3a9/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mirko/Projects/eudiplo/node_modules/.pnpm/vitest@3.2.4_@types+node@24.2.0_jiti@2.5.1_msw@2.10.4_@types+node@24.2.0_typescript@5.9_0c37c91df05756866e938c0c146bf3a9/node_modules/vitest/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/vitest@3.2.4_@types+node@24.2.0_jiti@2.5.1_msw@2.10.4_@types+node@24.2.0_typescript@5.9_0c37c91df05756866e938c0c146bf3a9/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
