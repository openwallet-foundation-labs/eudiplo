#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mirko/Projects/eudiplo/node_modules/.pnpm/typeorm@0.3.25_mysql2@3.14.3_pg@8.16.3_reflect-metadata@0.2.2_sqlite3@5.1.7_ts-node@10._e04f3e84c66fe6f54221a26eb2507b2f/node_modules/typeorm/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/typeorm@0.3.25_mysql2@3.14.3_pg@8.16.3_reflect-metadata@0.2.2_sqlite3@5.1.7_ts-node@10._e04f3e84c66fe6f54221a26eb2507b2f/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mirko/Projects/eudiplo/node_modules/.pnpm/typeorm@0.3.25_mysql2@3.14.3_pg@8.16.3_reflect-metadata@0.2.2_sqlite3@5.1.7_ts-node@10._e04f3e84c66fe6f54221a26eb2507b2f/node_modules/typeorm/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/typeorm@0.3.25_mysql2@3.14.3_pg@8.16.3_reflect-metadata@0.2.2_sqlite3@5.1.7_ts-node@10._e04f3e84c66fe6f54221a26eb2507b2f/node_modules:/Users/mirko/Projects/eudiplo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-commonjs.js" "$@"
fi
